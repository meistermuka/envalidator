{
  "PORT": {
    "type": "number",
    "required": true,
    "allowedValues": [
      3000,
      4001,
      9001
    ]
  },
  "HOST": {
    "type": "String",
    "required": "yes"
  },
  "LOG_LEVEL": {
    "type": "String",
    "required": 0,
    "defaultValue": "info",
    "allowedValues": [
      "debug",
      "info",
      "verbose",
      "warning"
    ]
  }
}
